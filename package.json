{
  "name": "teblo-monorepo",
  "version": "1.0.0",
  "description": "App de facturaci√≥n profesional - Monorepo",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
